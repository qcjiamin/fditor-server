{
  "name": "fditor-server",
  "version": "1.0.15",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start1": "ts-node-dev --respawn src/app.ts",
    "start": "cross-env NODE_ENV=development node --watch --loader ts-node/esm --experimental-specifier-resolution=node src/server.ts",
    "start11": "cross-env NODE_ENV=development node --env-file .env --watch --loader ts-node/esm --experimental-specifier-resolution=node src/server1.ts",
    "start22": "cross-env NODE_ENV=development node --env-file .env --watch --loader ts-node/esm --experimental-specifier-resolution=node src/server2.ts",
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "pmstart": "pm2 start ecosystem.config.cjs --env development",
    "pmstop": "pm2 stop ecosystem.config.cjs",
    "pmrestart": "pm2 restart ecosystem.config.cjs",
    "pmlogs": "pm2 logs",
    "pmclear": "pm2 stop ecosystem.config.cjs && pm2 delete all",
    "deploy": "node build.js",
    "pm-docker-start": "pm2-runtime ecosystem.config.cjs"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "bullmq": "^5.56.9",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "errorhandler": "^1.5.1",
    "express": "^5.1.0",
    "express-validator": "^7.2.1",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "mysql2": "^3.14.2",
    "pm2": "^6.0.8",
    "uuid": "^11.1.0",
    "zod": "^4.0.13"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/errorhandler": "^1.5.3",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.0.14",
    "@types/split2": "^4.2.3",
    "cross-env": "^7.0.3",
    "dotenv": "^17.2.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}
